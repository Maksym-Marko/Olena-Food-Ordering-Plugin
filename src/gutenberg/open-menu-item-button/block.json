{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "vajofo/open-menu-item-button",
	"version": "0.1.0",
	"title": "Olena. Open Menu Item Button",
	"category": "widgets",
	"icon": "smiley",
	"description": "Olena. Open Menu Item Button",
	"supports": {
		"html": false,
		"align": [
			"left",
			"right",
			"full"
		],
		"color": {
			"__experimentalDefaultControls": {
				"background": true,
				"text": true
			}
		}
	},
	"attributes": {
		"align": {
			"type": "string",
			"default": ""
		},
		"message": {
			"type": "string",
			"source": "text",
			"selector": "div",
			"default": "Hello, World!"
		}
	},
	"textdomain": "olena-food-ordering-source",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}